Algoritmo Ejercicio1
	Definir arreglo,i Como Entero
	Dimension arreglo(5)
	
	Para i<-0 Hasta 4 Con Paso 1 Hacer
		Escribir "Ingrese el valor de la posición ",i
		Leer arreglo(i)
	FinPara
	
	Escribir "Los valores ingresados son:"
	
	Para i=0 Hasta 4 Con Paso 1 Hacer
		Escribir "valor en posicion ",i,":",arreglo(i)
	FinPara
	
FinAlgoritmo
